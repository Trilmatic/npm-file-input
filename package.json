{
  "name": "@trilmatic/file-input",
  "version": "1.0.0",
  "description": "Vuejs plugin for form file input",
  "main": "dist/file-input.umd.js",
  "module": "dist/file-input.esm.js",
  "unpkg": "dist/file-input.min.js",
  "style": "dist/file-input.css",
  "browser": {
    "./sfc": "src/file-input.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/file-input.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/file-input.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/file-input.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.13",
    "vue-template-compiler": "^2.6.14"
  },
  "repository":"https://github.com/Trilmatic/npm-file-input",
  "keywords": [
    "form",
    "forms",
    "file",
    "input",
    "dropzone",
    "filedrop",
    "vuejs",
    "vue"
  ],
  "author": "Trilmatic",
  "license": "MIT"
}
